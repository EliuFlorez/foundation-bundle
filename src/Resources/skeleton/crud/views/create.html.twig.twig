{{ "{% extends '@foundation/base.html.twig' %}" }}

{{ "{% block content %}" }}

    <h1>Create new {{ entity_class_name }}</h1>

    {{ '{{ form_start(form) }}' }}

        {{ '{{ form_widget(form) }}' }}

        <button type="submit" class="button">
            <i class="fa fa-save"></i>
            Create
        </button>
        <a class="button" href="{{ "{{ app.request.headers.get('referer') }}" }}">
            <i class="fa fa-times"></i>
            Cancel
        </a>
    {{ '{{ form_end(form) }}' }}

{{ "{% endblock %}" }}
{% endblock body %}
